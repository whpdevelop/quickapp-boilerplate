    { "name": "system.prompt" },
    { "name": "system.shortcut" }
  ],
  "permissions": [
    { "origin": "*" }
  ],
  "config": {
    "designWidth": "750",
    "data": {},
    "logLevel": "debug"
  },
  "router": {
